//4. Write a C program to add two numbers, take number from user.

#include <stdio.h>
int main()
{
    int a,b,sum;
    
    printf("//Hansika Gupta\n");
    printf("Enter first no.");
    scanf("%d",&a);
    
    printf("Enter second no.");
    scanf("%d",&b);
    
    sum=a+b;
    
    printf("Addition: %d\n",sum);
    return 0;
}
